{
  "manifest_version": 2,

  "name": "Solshal",
  "description": "The easiest way to bookmark and organize links. Be productive.",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://solshal.com/"
  ]
}
