{
  "manifest_version": 2,
  "name": "Solshal",
  "description": "You are what you read. Discover who your friends are..",
  "version": "2.0.0",
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "http://solshal.com/",
    "http://localhost/*",
    "bookmarks"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
